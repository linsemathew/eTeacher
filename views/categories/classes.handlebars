<br>
<div class="container">
	<h2 class="all-classes">All Classes</h2>
	<div class="row">
		{{#each classes}}
			<div class="col-md-4">
				<h2 class="description">{{title}}</h2>
				<h4 class="description">with {{instructor}}</h4><br>
				<p class="description">{{description}}</p>
				<p><a class="btn btn-primary btn-lg" href="/classes/{{_id}}" role="button">View details &raquo;</a></p><br><br>
			</div>
		{{/each}}
	</div>
</div>