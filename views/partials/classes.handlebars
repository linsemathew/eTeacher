<div class="container">
	<div class="row">
		{{#each classes}}
		<div class="col-md-4">
			<img src="/images/new.png" class="new-tag"><br>
			<h2 class="description">{{title}}</h2>
			<h4 class="description">with {{instructor}}</h4><br>
			<p class="description">{{description}}</p>
			<p><a class="btn btn-primary btn-lg" href="/classes/{{_id}}/details" role="button">View details &raquo;</a></p><br><br>
		</div>
		{{/each}}
	</div>
</div>