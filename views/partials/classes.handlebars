<div class="container">
	<h2 class="new-classes">New Classes</h2>
	<div class="row">
		{{#each classes}}
		<div class="col-md-4">
			<h2>{{title}}</h2>
			<h4>with {{instructor}}</h4><br>
			<p class="description">{{description}}</p>
			<p><a class="btn btn-default" href="/classes/{{_id}}/details" role="button">View details &raquo;</a></p>
		</div>
		{{/each}}
	</div>
</div>