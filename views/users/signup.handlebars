<br>
<div class="container">
  <div class="panel panel-primary" id="entire-signup-form" action="/users/signup">
    <div class="panel-heading">
      <h2 class="form-title">Signup</h2>
    </div>
    <form class="signup-form" method="post">
      <div class="form-group row panel-body">
        <label for="account-type" class="col-sm-2 col-form-label">Account Type:</label>
        <div class="col-sm-10">
          <label class="form-check-inline">
            <input class="form-check-input" type="radio" name="student" id="inlineRadio1" value="student" checked> Student &nbsp;
          </label>
          <label class="form-check-inline">
            <input class="form-check-input" type="radio" name="instructor" id="inlineRadio2" value="instructor"> Instructor
          </label>
        </div>
      </div>
      <div class="form-group row panel-body">
        <label for="first_name" class="col-sm-2 col-form-label" value="{{first_name}}">First Name: </label>
        <div class="col-sm-10">
          <input type="text" class="form-control" id="inputEmail3" placeholder="First Name">
        </div>
      </div>
      <div class="form-group row panel-body">
        <label for="last_name" class="col-sm-2 col-form-label" value="{{last_name}}">Last Name: </label>
        <div class="col-sm-10">
          <input type="text" class="form-control" id="inputEmail3" placeholder="Last Name">
        </div>
      </div>
      <div class="form-group row panel-body">
        <label for="email" class="col-sm-2 col-form-label" value="{{email}}">Email: </label>
        <div class="col-sm-10">
          <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
        </div>
      </div>
      <div class="form-group row panel-body">
        <label for="password" class="col-sm-2 col-form-label" value="{{password}}">Password: </label>
        <div class="col-sm-10">
          <input type="password" class="form-control" id="inputPassword3" placeholder="Password">
        </div>
      </div>
      <div class="form-group row panel-body">
        <label for="password2" class="col-sm-2 col-form-label" value="{{password2}}">Confirm Password: </label>
        <div class="col-sm-10">
          <input type="password" class="form-control" id="inputPassword3" placeholder="Confirm Password">
        </div>
      </div>
      <div class="form-group row panel-body submit-button">
        <button type="submit" class="btn btn-primary btn-lg btn-block">submit</button>
      </div>
    </form>
  </div>
</div>