<div class="container">
	<h3>{{student.first_name}}'s Classes</h3>
	<table class="table table-striped">
		<thead>
			<tr class="info">
				<th>Class Name</th>
				<th>Instructor</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			{{#each student.classes}}
				<tr class="active">
					<td>{{class_title}}</td>
					<td>{{class_instructor}}</td>
					<td><a href="/classes/{{class_id}}/lessons">View Lessons</a></td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>